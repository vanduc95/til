[DEFAULT]
core_plugin = ml2
service_plugins =
auth_strategy = keystone    
notify_nova_on_port_status_changes = True
notify_nova_on_port_data_changes = True  
allow_overlapping_ips = True 
rpc_backend = rabbit
dhcp_agents_per_network = 2

[agent]

[cors]

[cors.subdomain]

[database]
connection = mysql+pymysql://neutron:{{ PASS_DATABASE_NEUTRON }}@{{ IP_VIP_API }}/neutron

[keystone_authtoken]
auth_uri = http://{{ IP_VIP_API }}:5000
auth_url = http://{{ IP_VIP_API }}:35357
memcached_servers = {{ CTL1_IP_NIC1 }}:11211,{{ CTL2_IP_NIC1 }}:11211,{{ CTL3_IP_NIC1 }}:11211
auth_type = password
project_domain_name = Default
user_domain_name = Default
project_name = service
username = neutron
password = {{ NEUTRON_PASS }}
[matchmaker_redis]

[nova]
auth_url = http://{{ IP_VIP_API }}:35357
auth_type = password
project_domain_name = Default
user_domain_name = Default
region_name = RegionOne
project_name = service
username = nova
password = {{ NOVA_PASS }}

[oslo_concurrency]
lock_path = /var/lib/neutron/tmp

[oslo_messaging_amqp]

[oslo_messaging_notifications]

[oslo_messaging_rabbit]
rabbit_hosts = {{ MQ1_IP_NIC1 }}:5672,{{ MQ2_IP_NIC1 }}:5672,{{ MQ3_IP_NIC1 }}:5672
rabbit_ha_queues = true
rabbit_retry_interval = 1
rabbit_retry_backoff = 2
rabbit_max_retries = 0
rabbit_durable_queues = true
rabbit_userid = openstack
rabbit_password = {{ RABBIT_PASS }}

[oslo_messaging_zmq]		
[oslo_middleware]
[oslo_policy]
[qos]
[quotas]

[ssl]
